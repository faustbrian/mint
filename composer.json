{
    "name": "cline/mint",
    "description": "A comprehensive unique identifier generation library supporting UUID, ULID, Snowflake, NanoID, Sqids, KSUID, CUID2, TypeID, XID, ObjectID, PushID, and Timeflake",
    "license": "MIT",
    "keywords": [
        "php",
        "uuid",
        "ulid",
        "snowflake",
        "nanoid",
        "sqids",
        "hashids",
        "ksuid",
        "cuid",
        "typeid",
        "xid",
        "objectid",
        "pushid",
        "timeflake",
        "identifier",
        "id-generator",
        "unique-id"
    ],
    "authors": [
        {
            "name": "Brian Faust",
            "email": "brian@cline.sh"
        }
    ],
    "require": {
        "php": "^8.5.0",
        "illuminate/contracts": "^11.0 || ^12.0",
        "illuminate/support": "^11.0 || ^12.0",
        "spatie/laravel-package-tools": "^1.92.7"
    },
    "require-dev": {
        "cline/coding-standard": "^1.6.3",
        "orchestra/testbench": "^8.0 || ^9.0 || ^10.8",
        "symfony/var-dumper": "^6.0 || ^7.4"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Cline\\Mint\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ergebnis/composer-normalize": true,
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "aliases": {
                "Mint": "Cline\\Mint\\Mint"
            },
            "providers": [
                "Cline\\Mint\\MintServiceProvider"
            ]
        }
    },
    "scripts": {
        "lint": "vendor/bin/ecs check --fix",
        "refactor": "rector",
        "test": [
            "@test:lint",
            "@test:type-coverage",
            "@test:unit",
            "@test:types",
            "@test:refactor"
        ],
        "test:lint": "vendor/bin/ecs check",
        "test:refactor": "rector --dry-run",
        "test:type-coverage": "pest --parallel --min=80 --type-coverage --memory-limit=512M",
        "test:types": "phpstan --memory-limit=512M",
        "test:unit": "pest --parallel --min=80 --coverage"
    }
}
